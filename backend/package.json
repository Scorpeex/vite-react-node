{
  "name": "@scorpeex/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prettify": "prettier --log-level log --cache --write {src/**/*,*}.{ts,tsx,js,json,yml,scss}",
    "dev": "ts-node-dev --watch ./.env --rs --exit-child ./src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts --fix",
    "start": "node ./dist/index.js",
    "pmd": "prisma migrate dev",
    "pgc": "prisma generate"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@prisma/client": "5.2.0",
    "@trpc/server": "10.37.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "superjson": "1.13.1",
    "ts-node-dev": "2.0.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.198",
    "@types/node": "20.5.0",
    "prisma": "5.2.0"
  }
}
